{
  "name": "reading-backend-managment-books",
  "version": "1.0.0",
  "description": "",
  "main": "back.js",
  "scripts": {
    "server": "nodemon back.js",
    "client": "npm start --prefix client",
    "start": "concurrently \"npm run server\" \"npm run client\"",
    "install-client": "cd client && npm install",
    "dev": "concurrently \"npm run start\" \" npm --prefix ./client run start\"",
    "heroku-postbuild": "npm run install-client && npm install && npm run build",
    "migrate:make": "knex migrate:make",
    "migrate:latest": "knex migrate:latest",
    "migrate:rollback": "knex migrate:rollback",
    "seed:make": "knex seed:make",
    "seed:run": "knex seed:run"
  },
  "author": "Joelle Maximin <joelle.maximin@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "bcrypt": "^4.0.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.1",
    "concurrently": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.18.2",
    "express-openid-connect": "^1.0.1",
    "express-session": "^1.17.1",
    "helmet": "^3.21.2",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.20.8",
    "morgan": "^1.9.1",
    "mysql": "^2.17.1",
    "router": "^1.3.3"
  },
  "devDependencies": {
    "nodemon": "^2.0.2"
  }
}
